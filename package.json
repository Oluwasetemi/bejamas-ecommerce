{
	"name": "bejamas-ecommerce",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next -p 8001",
		"build": "next build",
		"start": "next start -p 8001",
		"prestart": "next build",
		"lint:eslint": "eslint . --ext ts,tsx,js",
		"lint:prettier": "prettier --write \"**/*.{js,json,ts,tsx}\"",
		"lint:types": "tsc",
		"lint:css": "stylelint './src/**/*.(ts|tsx)'",
		"lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:types",
		"validate": "yarn lint && yarn test:unit -u",
		"test:coverage": "yarn test:unit --coverage",
		"test:watch": "yarn test:unit --watch",
		"test:unit": "jest --passWithNoTests",
		"test:all": "run-s test test:e2e",
		"test": "is-ci \"test:coverage\" \"test:watch\" ",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
		"test:e2e:run": "run-p -r dev cy:run",
		"test:e2e:dev": "start-server-and-test start 8001 cy:open",
		"test:e2e:ci": "cypress run",
		"prepare": "husky install",
		"lintstaged": "lint-staged"
	},
	"dependencies": {
		"@apollo/client": "^3.4.15",
		"@apollo/link-error": "^2.0.0-beta.3",
		"@apollo/react-ssr": "^4.0.0",
		"@emotion/css": "^11.1.3",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@types/react-modal": "^3.12.1",
		"apollo-upload-client": "^16.0.0",
		"focus-trap-react": "^8.8.1",
		"graphql": "^15.6.0",
		"graphql-tag": "^2.12.5",
		"next": "11.1.2",
		"next-with-apollo": "^5.2.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-modal": "^3.14.3"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.15.0",
		"@emotion/babel-plugin": "^11.3.0",
		"@emotion/eslint-plugin": "^11.2.0",
		"@emotion/jest": "^11.3.0",
		"@testing-library/cypress": "^8.0.1",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.1",
		"@testing-library/user-event": "^13.2.1",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.10.2",
		"@types/react": "17.0.26",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"babel-jest": "^27.2.4",
		"cypress": "^8.5.0",
		"eslint": "7.32.0",
		"eslint-config-next": "11.1.2",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-jest": "^24.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-prettier-config": "^1.0.1",
		"husky": "^7.0.2",
		"identity-obj-proxy": "^3.0.0",
		"is-ci-cli": "^2.2.0",
		"jest": "^27.2.4",
		"jest-watch-typeahead": "^1.0.0",
		"lint-staged": "^11.1.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.4.1",
		"start-server-and-test": "^1.14.0",
		"typescript": "4.4.3"
	},
	"prettier": {
		"singleQuote": true,
		"semi": true,
		"trailingComma": "all",
		"useTabs": true,
		"arrowParens": "always"
	}
}
